{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "prefer-arrow-callback": "warn",
        "no-var": "warn"
    },
    "globals": {
        "describe": "readonly",
        "it": "readonly"
    }
}
